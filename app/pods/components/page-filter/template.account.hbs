        </div>
        
        <div class="form-group col-md-6">
            <label for="objectif">Email</label>
            {{input value=spec.email class="form-control" type="text"}}
        </div>

    </div>
    
    <div class="row">

        <div class="form-group col-md-6">
            <label for="objectif">Organismes</label>
            {{#async-filter-input uniqueSelection=spec.selectedOrganisme selectionMaxLength=1 queryMinLength=0 endpoint="lexique/organisme/list" as |part o this| }}
